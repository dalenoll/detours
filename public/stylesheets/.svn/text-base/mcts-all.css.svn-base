/* ====================================================================
3D look tables
-------------------------------------------------------------------- */
TABLE.grid3d {BORDER-RIGHT: #999999 1px solid; BORDER-TOP: #cccccc 1px solid; MARGIN: 0px 0px 0.5em 2em; BORDER-LEFT: #cccccc 1px solid; BORDER-BOTTOM: #999999 1px solid; BACKGROUND-COLOR: #f0f7ff}
TABLE.grid3d TD {BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #ffffff 1px solid; BORDER-LEFT: #ffffff 1px solid; BORDER-BOTTOM: #cccccc 1px solid}
TABLE.grid3d TH {BORDER-RIGHT: #999999 1px solid; BORDER-TOP: #e0e7f7 1px solid; BORDER-LEFT: #e0e7f7 1px solid; BORDER-BOTTOM: #999999 1px solid; BACKGROUND-COLOR: #c7d0e0}
TABLE.grid3d TH.tfoot {BORDER-RIGHT: #999999 1px solid; BORDER-TOP: #e0e7f7 1px solid; BORDER-LEFT: #e0e7f7 1px solid; BORDER-BOTTOM: #999999 1px solid; BACKGROUND-COLOR: #c7d0e0}
TABLE.grid3d TBODY TR.e TH {BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #ffffff 1px solid; BORDER-LEFT: #ffffff 1px solid; BORDER-BOTTOM: #cccccc 1px solid; BACKGROUND-COLOR: #e0e7f7}
TABLE.grid3d TBODY TR.o TH {BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #ffffff 1px solid; BORDER-LEFT: #ffffff 1px solid; BORDER-BOTTOM: #cccccc 1px solid; BACKGROUND-COLOR: #e0e7f7}
TABLE.grid3d TR.e TD {BACKGROUND-COLOR: #e7f0fa; TEXT-ALIGN: center}
TABLE.grid3d TR.o TD {BACKGROUND-COLOR: #f0f7ff; TEXT-ALIGN: center}
TABLE.grid3d TR TD.left {TEXT-ALIGN: left}
TABLE.grid3d TR TH.left {TEXT-ALIGN: left}
TABLE.grid3d TR.tfoot TH.left {TEXT-ALIGN: left}
TABLE.grid3d THEAD TR TH {FONT-WEIGHT: bold; TEXT-ALIGN: center}
TABLE.grid3d TR.tfoot TH {FONT-WEIGHT: bold; TEXT-ALIGN: center}
TABLE.grid3d TBODY.left TD {TEXT-ALIGN: left}
TABLE.sidebar3d {MARGIN: 0px 0px 0.5em; BORDER-LEFT: #fff 0.5ex solid; BORDER-TOP-STYLE: none; BORDER-RIGHT-STYLE: none; BACKGROUND-COLOR: #f0f7ff; BORDER-BOTTOM-STYLE: none}
TABLE.sidebar3d TD {BORDER-RIGHT: #999999 1px solid; PADDING-RIGHT: 0.5ex; BORDER-TOP: #cccccc 1px solid; PADDING-LEFT: 0.5ex; PADDING-BOTTOM: 0.5ex; BORDER-LEFT: #cccccc 1px solid; PADDING-TOP: 0.5ex; BORDER-BOTTOM: #999999 1px solid}
TABLE.sidebar3d P {MARGIN: 0px 0px 0.3em; LINE-HEIGHT: 1.1em}
TABLE.sidebar {BORDER-LEFT: white 0.5em solid; BORDER-TOP-STYLE: none; BORDER-RIGHT-STYLE: none; BORDER-BOTTOM-STYLE: none}
TABLE.sidebar P.last {MARGIN-BOTTOM: 0px}
TABLE.right {left-margin: 1em; bottom-margin: 1ex}

